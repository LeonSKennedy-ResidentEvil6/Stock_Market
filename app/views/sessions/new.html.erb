<section class="stockmarketapp">
    <header class="header">
        <h1>Login</h1>
        <%= form_tag(sessions_path) do %>
            <h2>Username: <%= text_field_tag :username %></h2>
            <br><br>
            <h2>Email: <%= text_field_tag :email %></h2>
            <br><br>
            <h2>Password: <%= password_field_tag :password %></h2>
            <br><br>
            <%= submit_tag "Sign in"%>
        <% end %>
    </header>

    <footer class="footer">
    </footer>
</section>





